{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    },
    "automaticDeployments": true,
    "production": {
        "deploymentEnabled": false
    },
    "preview": {
        "deploymentEnabled": true
    }
}