{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "github": {
        "enabled": false,
        "autoJobCancelation": true
    },
    "automaticDeployments": false
}